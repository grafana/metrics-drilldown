{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.validate": [
    "typescript",
    "typescriptreact"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#8984ed",
    "activityBar.background": "#8984ed",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f7cac7",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#8984ed",
    "statusBar.background": "#5e58e6",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#8984ed",
    "statusBarItem.remoteBackground": "#5e58e6",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#5e58e6",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#5e58e699",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#5e58e6"
}
