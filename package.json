{
  "name": "grafana-metricsdrilldown-app",
  "version": "1.0.7",
  "author": "Grafana",
  "license": "AGPL-3.0",
  "scripts": {
    "prebuild": "./scripts/update-commit-sha.sh",
    "build": "webpack -c ./webpack.config.ts --env production",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "analyze": "webpack -c ./webpack-analyze.config.ts --env production",
    "tdd": "jest --watch --onlyChanged",
    "test": "jest --maxWorkers 4 --coverage",
    "test:ci": "jest --maxWorkers 4 --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "e2e:server": "docker compose up --build",
    "e2e:ci": "docker compose --profile playwright up --build --attach playwright --exit-code-from playwright",
    "e2e:prepare": "npm exec playwright install chromium",
    "e2e": "playwright test -c ./e2e/config/playwright.config.local.ts",
    "e2e:watch": "npm run e2e -- --ui",
    "e2e:codegen": "playwright codegen",
    "e2e:screenshots:regen": "./scripts/e2e-regen-screenshots.sh"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@bsull/augurs": "^0.9.0",
    "@emotion/css": "11.13.5",
    "@grafana/data": "^12.0.0",
    "@grafana/faro-web-sdk": "^1.17.2",
    "@grafana/lezer-logql": "^0.2.7",
    "@grafana/prometheus": "^12.1.0-252095",
    "@grafana/runtime": "^12.0.0",
    "@grafana/scenes": "^6.10.2",
    "@grafana/schema": "^12.0.0",
    "@grafana/ui": "^12.0.0",
    "@lezer/common": "^1.2.3",
    "@prometheus-io/lezer-promql": "^0.305.0-rc.1",
    "p-limit": "^6.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.30.0",
    "rxjs": "7.8.1",
    "tsqtsq": "^1.0.10"
  },
  "devDependencies": {
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^1.19.9",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.54.0",
    "@stylistic/eslint-plugin-ts": "^2.13.0",
    "@swc/core": "^1.11.24",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.38",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "14.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.32",
    "@types/testing-library__jest-dom": "5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.11.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsdoc": "^46.10.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.4.5",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.8.8",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.87.0",
    "sass-loader": "13.3.3",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.14",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "webpack": "^5.99.7",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  }
