{
  "name": "grafana-metricsdrilldown-app",
  "version": "1.0.28",
  "author": "Grafana",
  "license": "AGPL-3.0",
  "scripts": {
    "prebuild": "./scripts/update-commit-sha.sh",
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "analyze": "webpack -c ./webpack-analyze.config.ts --env production",
    "tdd": "jest --watch --onlyChanged",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint -- --fix && prettier --write --list-different .",
    "server": "docker compose up --build",
    "sign": "pnpm dlx @grafana/sign-plugin@latest",
    "e2e:server": "COMPOSE_BAKE=true docker compose up --build",
    "e2e:server:up": "COMPOSE_BAKE=true docker compose up --build -d",
    "e2e:server:down": "COMPOSE_BAKE=true docker compose down",
    "e2e:ci": "COMPOSE_BAKE=true docker compose --profile playwright up --build --attach playwright --exit-code-from playwright",
    "e2e:prepare": "pnpm exec playwright install chromium",
    "e2e:report": "playwright show-report e2e/test-reports",
    "e2e": "playwright test -c ./e2e/config/playwright.config.local.ts",
    "e2e:watch": "pnpm run e2e -- --ui",
    "e2e:codegen": "playwright codegen",
    "e2e:screenshots:gen": "./scripts/e2e-gen-screenshots.sh",
    "e2e:smoke": "playwright test -c ./e2e/config/playwright.config.smoke.ts -x --trace on",
    "knip": "pnpm dlx knip --config knip.js",
    "i18n:extract": "i18next -c i18next-parser.config.ts",
    "i18n:extract:check": "i18next -c i18next-parser.config.ts --fail-on-update"
  },
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@bsull/augurs": "^0.10.0",
    "@emotion/css": "11.13.5",
    "@grafana/assistant": "^0.1.0",
    "@grafana/data": "^12.3.0",
    "@grafana/faro-web-sdk": "^2.0.2",
    "@grafana/i18n": "^12.3.0",
    "@grafana/lezer-logql": "^0.2.7",
    "@grafana/prometheus": "^12.3.0",
    "@grafana/runtime": "^12.3.0",
    "@grafana/scenes": "^6.39.0",
    "@grafana/schema": "^12.3.0",
    "@grafana/ui": "^12.3.0",
    "@lezer/common": "^1.2.3",
    "@openfeature/ofrep-web-provider": "^0.3.5",
    "@openfeature/web-sdk": "^1.7.2",
    "@prometheus-io/lezer-promql": "^0.308.0",
    "leven": "^4.0.0",
    "p-limit": "^6.2.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-inlinesvg": "^4.2.0",
    "react-router-dom": "^6.30.0",
    "rxjs": "7.8.2",
    "semver": "^7.7.3",
    "tsqtsq": "^1.1.0"
  },
  "devDependencies": {
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "1.19.9",
    "@grafana/tsconfig": "2.0.1",
    "@playwright/test": "1.57.0",
    "@stylistic/eslint-plugin-ts": "2.13.0",
    "@swc/core": "1.14.0",
    "@swc/helpers": "0.5.17",
    "@swc/jest": "0.2.39",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "14.3.1",
    "@types/jest": "29.5.14",
    "@types/node": "20.19.25",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "6.11.0",
    "dotenv": "16.6.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "8.10.2",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "28.14.0",
    "eslint-plugin-jsdoc": "46.10.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "2.2.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-webpack-plugin": "4.2.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "11.1.0",
    "globals": "^15.0.0",
    "i18next-cli": "^1.34.1",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "2.8.8",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.93.3",
    "sass-loader": "13.3.3",
    "style-loader": "3.3.4",
    "swc-loader": "0.2.6",
    "terser-webpack-plugin": "5.3.14",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "webpack": "5.102.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "peerDependencies": {
    "@react-aria/utils": "3.31.0"
  }
}
